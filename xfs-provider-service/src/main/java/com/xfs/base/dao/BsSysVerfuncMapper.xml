<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="BS_SYS_VERFUNC" >
<sql id="Base_Column_List" >
	id,ver_id,func_type,func_info,rank,create_by,create_dt,modify_by,modify_dt,dr
</sql>
    <!-- 查询条件 -->
    <sql id="Example_Where_Clause">
        <trim prefix=" WHERE " prefixOverrides="AND">
            <if test="id != null" >and id = #{id}</if>
            <if test="ver_id != null" >and ver_id = #{ver_id}</if>
            <if test="func_type != null" >and func_type = #{func_type}</if>
            <if test="func_info != null" >and func_info = #{func_info}</if>
            <if test="rank != null" >and rank = #{rank}</if>
            <if test="create_dt != null" >and create_dt = #{create_dt}</if>
            <if test="dr != null" >and dr = #{dr}</if>
        </trim>
    </sql>


    <!-- 查询版本列表  -->
    <select id="FreeFindBS_SYS_VER_FUNC"  resultType="java.util.Map" parameterType="Object">
        SELECT <include refid="Base_Column_List" /> FROM BS_SYS_VERFUNC
        <include refid="Example_Where_Clause" />
        order by create_dt  asc
    </select>

</mapper>